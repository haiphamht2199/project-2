            </ul>
            <!-- /NAV -->
        </div>
        <!-- /responsive-nav -->
    </div>
    <!-- /container -->
</nav>
<!-- /NAVIGATION -->



<!-- BREADCRUMB -->
<div id="breadcrumb" class="section">
 <!-- container -->
 <div class="container">
     <!-- row -->
     <div class="row">
         <div class="col-md-12">
             <h3 class="breadcrumb-header">Kiểm tra</h3>
             <ul class="breadcrumb-tree">
                 <li><a href="/">Trang chủ</a></li>
                 <li class="active">Đặt hàng</li>
             </ul>
         </div>
     </div>
     <!-- /row -->
 </div>
 <!-- /container -->
</div>
<!-- /BREADCRUMB -->

<!-- SECTION -->
<div class="section">
 <!-- container -->
 <div class="container">
     <!-- row -->
     <div class="row">
         <form method="POST" action="/cart/editAddress/<%=data._id%>">
             <div class="col-md-7">
                 <!-- Billing Details -->
                 <div class="billing-details">
                     <div class="section-title">
                         <h3 class="title">Địa chỉ thanh toán và nhận hàng</h3>
                     </div>
                     <div class="form-group">
                         <input class="input" value="<%=data.nguoiNhan%>" type="text" name="nguoiNhan" placeholder="Tên">
                     </div>
                     <div class="form-group">
                         <input class="input" value="<%=data.sdtNguoiNhan%>" type="tel" name="sdtNguoiNhan" placeholder="Số điện thoại">
                     </div>
                     <div class="form-group">
                         <input class="input" value="<%=data.diaChiNhan%>" type="text" name="diaChiNhan" placeholder="Địa chỉ">
                     </div>
                     <div>
                         <input class="primary-btn order-submit" type="submit" value="Thay đổi"/>
                     </div>
                 </div>
                 <!-- /Billing Details -->
             </div>
             
         </form>
     </div>
     <!-- /row -->
 </div>
 <!-- /container -->
</div>
<!-- /SECTION -->

<!-- FOOTER -->
<footer id="footer">
    <!-- top footer -->
    <!-- bottom footer -->
    <div id="bottom-footer" class="section">
        <div class="container">
            <!-- row -->
            <div class="row">
                <div class="col-md-12 text-center">
                    <ul class="footer-payments">
                        <li><a href="#"><i class="fa fa-cc-visa"></i></a></li>
                        <li><a href="#"><i class="fa fa-credit-card"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-paypal"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-mastercard"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-discover"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-amex"></i></a></li>
                    </ul>
                    <span class="copyright">
