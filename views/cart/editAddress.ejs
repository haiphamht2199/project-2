<!-- HEADER -->
<%- include('../layouts/header'); -%>
<!-- /HEADER -->

<!-- NAVIGATION -->
<nav id="navigation">
    <!-- container -->
    <div class="container">
        <!-- responsive-nav -->
        <div id="responsive-nav">
            <!-- NAV -->
            <ul class="main-nav nav navbar-nav">
             <li><a><a href="/">Trang chủ</a></a></li>
                <li><a><a href="/khoahoc/all">Tất cả</a></a></li>
                <li><a><a href="/khoahoc/lop3">Lơp 3</a></a></li>
                <li><a><a href="/khoahoc/lop4">Lớp 4</a></a></li>
