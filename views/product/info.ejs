    <title>An Toàn điện</title>>

    
    <link type="text/css" rel="stylesheet" href="../../../stylesheets/css/bootstrap.min.css"/>

    <link type="text/css" rel="stylesheet" href="../../stylesheets/css/slick.css"/> -->
    <link type="text/css" rel="stylesheet" href="../../stylesheets/css/slick-theme.css"/>

 
    <link type="text/css" rel="stylesheet" href="../../stylesheets/css/nouislider.min.css"/>

    <link type="text/css" rel="stylesheet" href="../../stylesheets/css/style.css"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="../../vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">


    <link href="../../vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">
   

    <link href="../../css/sb-admin.css" rel="stylesheet"> -->


</head>
<body>
<!-- HEADER -->
<%- include('../layouts/header'); -%>
<!-- /HEADER -->

<!-- NAVIGATION -->
<nav id="navigation">
    <!-- container -->
    <div class="container">
        <!-- responsive-nav -->
        <div id="responsive-nav">
            <!-- NAV -->
            <ul class="main-nav nav navbar-nav">
                <li><a href="/">Trang chủ</a></li>
                <li><a href="/khoahoc/all">Tất cả</a></li>
                <li><a href="/khoahoc/lop3">Lớp 3</a></li>
                <li><a href="/khoahoc/lop4">Lớp 4</a></li>
                <li><a href="/khoahoc/lop5">Lớp 5</a></li>
            </ul>
            <!-- /NAV -->
        </div>
        <!-- /responsive-nav -->
    </div>
    <!-- /container -->
</nav>
<!-- /NAVIGATION -->

<!-- SECTION -->
<div id="breadcrumb" class="section">
 <!-- container -->
 <div class="container">
     <!-- row -->
     <div class="row">
         <div class="col-md-12">
             <ul class="breadcrumb-tree">
                 <li><a href="/">Trang chủ</a></li>
                 <li><a href="#"><%=data.loai%></a></li>
                 <li><a href="#"><%=data.ten%></a></li>
             </ul>
         </div>
     </div>
     <!-- /row -->
 </div>
 <!-- /container -->
</div>
<!-- /BREADCRUMB -->

<!-- SECTION -->
<div class="section">
 <!-- container -->
 <div class="container">
     <!-- row -->
     <div class="row">
         <!-- Product details -->
         <div class="col-md-12">
             <div class="product-details">
                 <h2 class="product-name">Tên khóa học: <%=data.ten%></h2>
                 <div>
                     <div class="product-rating">
                         <i class="fa fa-star"></i>
                         <i class="fa fa-star"></i>
                         <i class="fa fa-star"></i>
                         <i class="fa fa-star"></i>
                         <i class="fa fa-star-o"></i>
                     </div>
                 </div>
                 <div>
                     <h3 class="product-price">Giá: <%=data.gia%> VNĐ</h3>
                 </div>
                 <!-- <p>Mô tả: <%=data.hinhAnh%></p> -->
                
                 <div id="mota">
                     
                     <%- data.mota%>
                    <!-- <font color="#333333" face="Montserrat, sans-serif"><span style="font-size: 14px; background-color: rgb(255, 255, 255);"><b>Mục tiêu:</b></span></font><blockquote style="margin: 0 0 0 40px; border: none; padding: 0px;"><div><ul><li style="box-sizing: border-box;"><span style="box-sizing: border-box;">Nhận biết các vật mang điện, dẫn điện trong gia đình.</span></li><li style="box-sizing: border-box;"><span style="box-sizing: border-box;">Cách phòng tránh tai nạn điện&nbsp;</span></li></ul></div></blockquote> -->
                  </div>
                  <div value>

                  </div>
             
                 <div class="add-to-cart">
                     <a href="/cart/add/<%=data._id%>"><button class="add-to-cart-btn"><i class="fa fa-shopping-cart"></i>Đăng kí khóa học</button></a>
                 </div>
             </div>
         </div>
         <!-- /Product details -->

         <!-- Product tab -->
         <div class="col-md-12">
             <div id="product-tab">
                 <!-- product tab nav -->
                 <ul class="tab-nav">
                     <li class="active"><a data-toggle="tab" href="#tab2">Đánh giá (<%=numberComment%>)</a></li>
                 </ul>
                 <!-- /product tab nav -->

                 <!-- product tab content -->
                 <div class="tab-content">
                     <!-- tab2  -->
                     <div id="tab2" class="tab-pane fade in active">
                         <div class="row">
                             <!-- Rating -->
                             <div class="col-md-3">
                                 <div id="rating">
                                     <div class="rating-avg">
                                         <span><%=TbStar%></span>
                                         <div class="review-rating"> 
                                            <%for(var i=1;i<=TbStar;i++){%>
                                                <i class="fa fa-star"></i>
                                                
                                                <%}%>
                                                <%for(var i=1;i<= (5-TbStar);i++){%>
                                                    <%if(5-TbStar<1){%>
                                                        <i class="fa fa-star-o"></i>
                                                        <%}%>
                                                
                                                    
                                                    <%}%>  
                                          
                                        </div>
                                     </div>
                                     <ul class="rating">
                                         <li>
                                             <div class="rating-stars">
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                             </div>
                                             <span>: </span>
                                             <span class="sum"><%=star.numberSta5%></span>
                                         </li>
                                         <li>
                                             <div class="rating-stars">
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star-o"></i>
                                             </div>
                                             <span>: </span>
                                             <span class="sum"><%=star.numberSta4%></span>
                                         </li>
                                         <li>
                                             <div class="rating-stars">
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                             </div>
                                             <span>: </span>
                                             <span class="sum"><%=star.numberSta3%></span>
                                         </li>
                                         <li>
                                             <div class="rating-stars">
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                             </div>
                                             <span>: </span>
                                             <span class="sum"><%=star.numberSta2%></span>
                                         </li>
                                         <li>
                                             <div class="rating-stars">
                                                 <i class="fa fa-star"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                                 <i class="fa fa-star-o"></i>
                                             </div>
                                            <span>: </span>
                                             <span class="sum"><%=star.numberSta1%></span>
                                         </li>
                                     </ul>
                                 </div>
                             </div>
                             <!-- /Rating -->

                             <!-- Reviews -->
                             <div class="col-md-6"
                                    >
                                 
                                 <div id="reviews">
                                     <%if(numberComment){%>
                                        <%comments.forEach(comment=>{%>
                                            <ul class="reviews" id="comment">
                                                <li>
                                                    <div class="review-heading">
                                                        <h5 class="name"><%=comment.ho%><%=comment.ten%></h5>
                                                        <p class="date"><%=comment.dataTime%></p>
                                                        <div class="review-rating"> 
                                                            <%for(var i=1;i<=comment.numberStar;i++){%>
                                                                <i class="fa fa-star"></i>
                                                                
                                                                <%}%>
                                                                <%for(var i=1;i<= (5-comment.numberStar);i++){%>
                                                                   
                                                                    <i class="fa fa-star-o"></i>
                                                                    <%}%>  
                                                          
                                                        </div>
                                                    </div>
                                                    <div class="review-body">
                                                        <p><%=comment.content%></p>
                                                    </div>
                                                </li>
                                                
                                            </ul>
                                            <%}) %>
                                       
                                        <%} %>
                                 </div>                                   
                             </div>
                             <!-- /Reviews -->

                             <!-- Review Form -->
                             <%if(user._id){%>
                                <div class="col-md-3">
                                    <div id="review-form">
                                        <form class="review-form" method="POST" action="/comment/add/<%=data._id%>">
                                           
                                            <textarea class="input" name="nhanXet" placeholder="Đánh giá"required="required" autofocus="autofocus" ></textarea>
                                            <div class="input-rating">
                                                <span>Xếp hạng: </span>
                                                <div class="stars">
                                                    <input id="star5" name="rating" value="5" type="radio"><label for="star5"></label>
                                                    <input id="star4" name="rating" value="4" type="radio"><label for="star4"></label>
                                                    <input id="star3" name="rating" value="3" type="radio"><label for="star3"></label>
                                                    <input id="star2" name="rating" value="2" type="radio"><label for="star2"></label>
                                                    <input id="star1" name="rating" value="1" type="radio"><label for="star1"></label>
                                                </div>
                                            </div>
                                            <button class="primary-btn">Đánh giá</button>
                                        </form>
                                    </div>
                                </div>
                                <%}%>
                           
                             <!-- /Review Form -->
                         </div>
                     </div>
                     <!-- /tab2  -->
                 </div>
                 <!-- /product tab content  -->
             </div>
         </div>
         <!-- /product tab -->
     </div>
     <!-- /row -->
 </div>
 <!-- /container -->
</div>
<!-- /SECTION -->


<div class="section">

 <div class="container">
     
     <div class="row">

         <div class="col-md-12">
             <div class="section-title text-center">
                 <h3 class="title">Sản phẩm liên quan</h3>
             </div>
         </div>

       <%data1.forEach(product=>{%>
        <div class="col-md-3 col-xs-6">
            <div class="product">
                <div class="product-img">
                    <img src="<%=product.hinhAnh%>" alt="">
                </div>
                <div class="product-body">
                    <p class="product-category"><%=product.loai%></p>
                    <h3 class="product-name"><a href="/product/info/<%=product._id%>"><%=product.ten%></a></h3>
                    <h4 class="product-price"><%=product.gia%> VNĐ</h4>
                    <div class="product-rating">
                    </div>
                </div>
                <div class="add-to-cart">
                    <a href="/cart/add/<%=product._id%>"><button class="add-to-cart-btn"><i class="fa fa-shopping-cart"></i>Đăng kí khóa học</button></a>
                </div>
            </div>
        </div>
        <%})%>
         
           
         
     </div>
 
 </div>

</div>

</div>
<!-- FOOTER -->
<footer id="footer">
    <div id="bottom-footer" class="section">
        <div class="container">
            <!-- row -->
            <div class="row">
                <div class="col-md-12 text-center">
                    <ul class="footer-payments">
                        <li><a href="#"><i class="fa fa-cc-visa"></i></a></li>
                        <li><a href="#"><i class="fa fa-credit-card"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-paypal"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-mastercard"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-discover"></i></a></li>
                        <li><a href="#"><i class="fa fa-cc-amex"></i></a></li>
                    </ul>
                    <span class="copyright">
								<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
